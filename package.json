    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.conf.js --progress --hide-modules",
    "1-old-build": "webpack --config ./build/webpack.client.conf.js && webpack --config ./build/webpack.server.conf.js",
    "2-old-build": "webpack --config webpack.client.config.js && webpack --config webpack.server.config.js"
